{
  "name": "Markdown",
  "scopeName": "source.md",
  "patterns": [
    {
      "name": "Symbols",
      "match": "[*#-]+"
    },
    {
      "name": "Brackets",
      "match": "[{}]+"
    },
    {
      "name": "Strings",
      "match": "\"([^\"\\\\]|\\\\.)*\""
    },
    {
        "name": "Backquotes",
        "match": "`.*?`"
    }
  ]
}
